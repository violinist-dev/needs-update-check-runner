{
  "name": "violinist-dev/needs-update-check-runner",
  "description": "To check if a project needs update, in some way.",
  "autoload": {
    "psr-4": {
      "Violinist\\NeedsUpdateCheckRunner\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": { "Violinist\\NeedsUpdateCheckRunner\\Tests\\": "tests/"}
  },
  "config": {
    "platform": {
      "php": "7.1.9"
    }
  },
  "require": {
    "wa72/simplelogger": "^1.1",
    "violinist-dev/provider-factory": "dev-main",
    "violinist-dev/update-check-data": "dev-main",
    "codeaken/sshkey": "^1.2",
    "symfony/dependency-injection": "^3.3",
    "symfony/dotenv": "^3.4",
    "symfony/cache": "^3.4",
    "symfony/process": "^3.4",
    "violinist-dev/composer-cached-remote-filesystem": "dev-master"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "phpstan/phpstan": "^0.12.88",
    "stevenmaguire/oauth2-bitbucket": "^3.0"
  }
}
